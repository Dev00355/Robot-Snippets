{
    "name": "robot-snippets",
    "displayName": "Robot Snippets",
    "publisher": "DevPandey",
    "description": "Robot framework code snippets",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.38.0"
    },
	"repository": {
		"type": "git",
		"url": "https://github.com/Dev00355/Robot-Snippets.git"
	},
    "categories": [
        "Snippets"
    ],
    "extensionDependencies": [
        "tomiturtiainen.rf-intellisense"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "robot",
                "extensions":[
                    ".robot",
                    ".resource"
                ],
                "path": "./snippets/snippets.code-snippets"
            }
        ]
    }
}
